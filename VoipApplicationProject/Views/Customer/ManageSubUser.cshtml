<div class="modal fade " id="AddModal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header" style="text-align:center">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row" style="margin-left:35%;">
                    <div class="col-md-2" style="margin-left: 10px; height: 100px; border: 2px solid black; background-color: dodgerblue">
                        <label style="margin-top:40%;margin-left:5%"><a href="#" style="color: black; text-align: center" data-toggle="modal" data-target="#AddAgent" data-dismiss="modal">Add Agent</a></label>
                    </div>&nbsp;&nbsp;&nbsp;&nbsp;
                    <div class="col-md-2" style="margin-left: 10px; height: 100px; border: 2px solid black; background-color: yellowgreen;display:inline-block;text-align:center;">
                        <label style="margin-top:40%;margin-left:5%"><a href="#" style="color:black;text-align:center;" data-toggle="modal" data-target="#AddDevice" data-dismiss="modal">Add Device</a></label>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
<div style="margin-left:70%">
    <button type="submit" id="btn1" class="btn btn-primary" onclick="javascript: fnExcelReport('SubUserTable', 'data')">EXPORT TO EXCEL</button>
</div>

<div class="modal fade " id="AddAgent" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header" style="text-align:center">
                <h5 style="color:red;margin-left:35%;">Create Agent - Enter Following Fields</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" style="margin:0 30% 0 30%">
                <form>
                    <!-- Name input -->
                    <div class="form-outline mb-4">
                        <input type="text" id="agentname" class="form-control" placeholder="Enter Name" />

                    </div>
                    <!-- UserName input -->
                    <div class="form-outline mb-4">
                        <input type="text" id="agentusername" class="form-control" placeholder="Enter Username Name" />
                    </div>

                    <!-- Password input -->
                    <div class="form-outline mb-4">
                        <input type="password" id="agentpassword" class="form-control" placeholder="Enter Password" />

                    </div>

                    <div class="form-outline mb-4">
                        <textarea class="form-control" id="agenttextarea" rows="2" placeholder="Caller ID"></textarea>
                    </div>
                    <!-- Create button -->
                    <button type="submit" data-toggle="modal" data-target="#ConfirmAgent" class="btn btn-primary btn-block" data-dismiss="modal">Create</button>
                </form>
            </div>
        </div>
    </div>
</div>


