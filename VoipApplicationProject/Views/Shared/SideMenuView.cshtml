@model IEnumerable<VoipApplicationProject.Models.MenuAccessModel>
@{

    string AddSpace(VoipApplicationProject.Models.MenuLink men) => string.Concat(men.ToString().Select(x => Char.IsUpper(x) ? " " + x : x.ToString())).TrimStart(' ');
}

@if (ViewBag.menulist == null)
{
    @*<script>alert("No Menu Items For This Customer !");</script>*@
}
else
{
    <div class="sidenav border-right">  
        @foreach (var menu in ViewBag.menulist)
        {
            <a href="Customer/@menu.MenuLink" class="border-bottom menulinks" style=" color:white">@AddSpace(@menu.MenuLink)</a>
        }
    </div>
}
  

